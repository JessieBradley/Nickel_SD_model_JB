Number of projects=
	ELMCOUNT(Project)
	~	Dmnl
	~	Unit = projects, but dmnl is used to avoid unit errors.
	|

Averaging period=
	1
	~	Year
	~	Keep this at 1
	|

Min adjusted attractiveness=
	VMIN(Profit adjusted investment attractiveness ranking[Project!])
	~	Dmnl
	~		|

Max adjusted attractiveness=
	VMAX(Profit adjusted investment attractiveness ranking[Project!])
	~	Dmnl
	~		|

Normalised profit adjusted investment attractiveness ranking[Project]=
	(Profit adjusted investment attractiveness ranking[Project]-Min adjusted attractiveness\
		)/(Max adjusted attractiveness-Min adjusted attractiveness)
	~	Dmnl
	~		|

Project investment attractiveness[Project]=
	SUM(Project country[Mining country!,Project]*Investment attractiveness[Mining country\
		!])
	~	Dmnl
	~		|

Lower bound development time=
	5
	~	Year
	~		|

Upper bound development time=
	10
	~	Year
	~		|

Profit adjusted investment attractiveness ranking[Project]=
	Project investment attractiveness[Project]*Profit over investment ranking[Project]
	~	Dmnl
	~		|

Development time[Project]=
	IF THEN ELSE(Initial developing[Project]>0,(Lower bound development time+(1-Project investment attractiveness\
		[Project])*(Upper bound development time-Lower bound development time))/2,Lower bound development time\
		+(1-Project investment attractiveness
	[Project])*(Upper bound development time-Lower bound development time))
	~	Year
	~	Made this dependent on investment attractiveness, with the reasoning that \
		countries with a better investment climate often have less corruption and \
		other inefficiencies, thereby increasing the likelihood of faster \
		development. For mines that are already under development at the start of \
		the model, the development time is cut in half.
	|

