Width=
	1/Number of projects
	~	Dmnl
	~		|

Total existing capacity=
	SUM(Existing capacity[Project!])
	~	tonne/Year
	~		|

Global maximum capacity increase=
	Total existing capacity*Global maximum capacity increase percentage
	~	tonne/Year
	~		|

Sum added capacity=
	SUM(New brownfield capacity[Project!]+New greenfield capacity[Project!])
	~	tonne/Year
	~		|

Mothballed[Project]= INTEG (
	Parking[Project]-Decomissioning[Project]-Restarting[Project],
		"Initial C&M"[Project])
	~	tonne/Year
	~		|

Sum mothballed=
	SUM(Mothballed[Project!])
	~	tonne/Year
	~		|

Minimum profit surplus[Project]=
	Lower bound minimum profit surplus[Project]+(1-Profit over investment ranking[Project\
		])*(Upper bound minimum profit surplus[Project]-Lower bound minimum profit surplus[\
		Project])
	~	$
	~	Reasoning:lower for the projects making the most profit
	|

Maximum profit deficit[Project]=
	Lower bound maximum profit deficit[Project]+Profit over investment ranking[Project]*\
		(Upper bound maximum profit deficit[Project]-Lower bound maximum profit deficit[Project\
		])
	~	$
	~		|

Sum mined=
	SUM(Mined nickel stock[Project!])
	~	tonne
	~		|

Profit over investment ranking[Project]=
	Ranked profit over investment[Project]/Number of projects
	~	Dmnl
	~		|

Fuel price change=
	IF THEN ELSE(Switch price scenario=1,
	Energy price scenario low*Fuel price,
	
	IF THEN ELSE(Switch price scenario=2,
	Energy price scenario mid*Fuel price,
	
	Energy price scenario high*Fuel price))
	~	$/(GJ*Year)
	~		|

Project class[Metal quality,Project]=
	SUM(Process class[Metal quality,Principal process!]*Project process[Principal process\
		!,Project])
	~	Dmnl
	~	Indicates whether a certain project leads to the production of class 1 or \
		class 2 nickel
	|

Project region[Project,Regions]=
	SUM(Project country[Mining country!,Project]*Country regions[Mining country!,Regions\
		])
	~	Dmnl
	~	Indicates which region a certain project is located in
	|

Energy price scenario low= WITH LOOKUP (
	Time,
		([(2015,-0.2)-(2100,1)],(2015,-0.3),(2016,-0.2),(2017,0.5),(2018,0),(2020,-0.1),(2021\
		,-0.05),(2025, -0.04),(2030,-0.03),(2040,-0.02),(2050,-0.01),(2060,-0.005),(2070,-0.002\
		),(2080,-0.001),(2090,-0.0005),(2100,0) ))
	~	1/Year
	~		|

Energy price scenario high= WITH LOOKUP (
	Time,
		([(2015,-0.5)-(2100,0.5)],(2015,-0.3),(2016,-0.2),(2017,0.5),(2018,0),(2020,-0.1),(\
		2021,0.1),(2025,0.04),(2030,0.03),(2040,0.015),(2050,0.01),(2060,-0.003),(2070,-0.002\
		),(2080,-0.001),(2090,0),(2100,0) ))
	~	1/Year
	~		|

Process class[Metal quality,Principal process]=
	GET XLS CONSTANTS( 'Nickel-subset-4-BSprecherxx.xlsx' , 'Process class' , 'B3' )
	~	Dmnl
	~	Indicates whether a certain principal process leads to the production of \
		class 1 or class 2 nickel
	|

Class based demand[Class 1]=
	Final nickel demand[Batteries]+Final nickel demand[Other applications] ~~|
Class based demand[Class 2]=
	Final nickel demand[Stainless steel]
	~	tonne/Year
	~	A certain percent of stainless steel is currently also supplied by class 1 \
		(Gigametals, 2019). However, as it is expected that in the future battery \
		demand will grow, it is assumed most stainless steel will be produced from \
		class 2, so class 1 demand is assumed to consist only of battery and other \
		demand.
	|

Existing mines=
	SUM(Existance of mines[Project!])
	~	Dmnl
	~	The number of existing projects at a certain time. 
		Unit = projects, but dmnl is used to avoid unit errors.
		Can start decreasing at a certain period based on the maximum mothball time. This is \
		due to a discrepancy between current existing mines and the mines that \
		should exist according to the model.
		This value never reaches the number of mines in the database by Mudd \
		(2020) because there are many potential projects with insufficent initial \
		resources in the database. See the variable 'Potential projects with \
		insufficient initial resources'
	|

Existance of mines[Project]=
	IF THEN ELSE(Existing capacity[Project]>0,1,0)
	~	Dmnl
	~	Indicates whether a certain project already has existing capacity
	|

Sum processing=
	SUM(Smelting and refining[Project!])
	~	tonne/Year
	~		|

Sum final demand=
	SUM(Final nickel demand[Demand categories!])
	~	tonne/Year
	~		|

Marginal cost nickel[Project]=
	Energy costs mining[Project]*Nickel allocation[Project]+Unique part of marginal cost\
		[Project]
	~	$/tonne
	~		|

Fuel costs refining[Project]=
	Fuel price*SUM(Fuel use refining[Principal process!]*Project process[Principal process\
		!,Project])
	~	$/tonne
	~		|

Initial number of operating mines=
	SUM(Initial status[Operating,Project!])
	~	Dmnl
	~	Unit = projects, but dmnl is used to avoid unit errors.
	|

Average initial operating marginal costs without other and carbon price=
	SUM(Initial marginal costs without other and carbon price[Project!]*Initial status[Operating\
		,Project!])/Initial number of operating mines
	~	$/tonne
	~	Do the divided by thing differently
	|

Fuel demand for nickel transport[Principal process]=
	0,0,2,2,2,2,2,2,0,2,2
	~	GJ/tonne
	~	Eckelman (2010) Assumptions: HM and beneficiation = F, DSO = BF, undefined \
		= HPAL
	|

Fuel demand for refining[Principal process]=
	0,0,5.7,5.7,5.7,5.7,0,0,0,0,5.7
	~	GJ/tonne
	~	Eckelman (2010) Assumptions: HM and beneficiation = F, DSO = BF, undefined \
		= HPAL
	|

Fixed costs[Project]=
	Invested capital costs[Project]*Fixed costs as percentage of capital costs
	~	$/Year
	~		|

Energy costs mining[Project]=
	Electricity costs mining[Project]+Fuel costs mining[Project]
	~	$/tonne
	~		|

Nickel taxes=
	0.3
	~	Dmnl
	~	PWC (2012)
	|

Initial average total marginal costs=
	Initial nickel price/(1+Percentage cost on top of marginal cost)
	~	$/tonne
	~	Initial scarcity assumed to be 1.
	|

Electricity use mining[Mine type]= INTEG (
	Change in electricity use mining[Mine type],
		"Electricity demand for mining, milling and beneficiation"[Mine type])
	~	GJ/tonne ore
